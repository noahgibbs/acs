<h1>Listing room_states</h1>

<table>
  <tr>
    <th>Json data</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @room_states.each do |room_state| %>
  <tr>
    <td><%= room_state.json_data %></td>
    <td><%= link_to 'Show', room_state %></td>
    <td><%= link_to 'Edit', edit_room_state_path(room_state) %></td>
    <td><%= link_to 'Destroy', room_state, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

